<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mobaas.stfx.dao.mybatis.SysErrorMapper">

    <select id="findCount" resultType="java.lang.Integer">
        SELECT
        COUNT(id)
        FROM
        sys_error
    </select>

    <select id="findList" resultType="com.mobaas.stfx.entity.SysError">
        SELECT
        *
        FROM
        sys_error
        ORDER BY id DESC
        LIMIT #{offset}, #{limit}
    </select>
   
</mapper>